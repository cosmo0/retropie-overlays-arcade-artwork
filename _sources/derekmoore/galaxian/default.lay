<!-- galaxian.lay -->

<mamelayout version="2">

  <element name="Artwork">
    <image file="art.png" />
  </element>

  <element name="Artwork2">
    <image file="art2.png" />
  </element>

  <element name="Generic">
    <image file="generic.png" />
  </element>

  <element name="Generic2">
    <image file="generic2.png" />
  </element>

 <element name="screen_bezel">
    <image file="vert_screen_bezel.png" />
  </element>

   <view name="View_1">
    <screen index="0">
      <bounds x="555" y="0" width="810" height="1080" />
    </screen>

    <bezel element="Artwork">
